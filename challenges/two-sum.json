{
  "name": "two-sum",
  "difficulty": "easy",
  "description": "Given an array and a target number, return the indices of the first pair of numbers that add up to the target. Return as a list of two indices.",
  "function_name": "two_sum",
  "type": "function",
  "inputs": ["arr", "target"],
  "tests": [
    {
      "input": [[1, 4, 10, -3], 14],
      "expected_output": [1, 2]
    },
    {
      "input": [[2, 7, 11, 15], 9],
      "expected_output": [0, 1]
    },
    {
      "input": [[3, 2, 4], 6],
      "expected_output": [1, 2]
    },
    {
      "input": [[5, 5, 5, 5], 10],
      "expected_output": [0, 1],
      "hidden": true
    },
    {
      "input": [[-2, 7, 11, 15], 13],
      "expected_output": [0, 3],
      "hidden": true
    },
    {
      "input": [[1, 2, 3, 4, 5], 9],
      "expected_output": [3, 4]
    }
  ]
}
